.container {
    width: 100%;
    height: 100%;
    display: flex;
}

.container_left {
    width: 100px;
    font-size: 14px;
    background-color: #eeeeee;
    height: 100%;
    text-align: center;
}

.container_menu_list {
    padding: 12px 0;
}

.container_menu_list_active {
    font-size: 16px;
    color: pink;
    background-color: #ffffff;
}

.container_right {
    width: 400px
}

.container_right_body {
    margin: 0 8px;
}

.container_right_header {
    margin: 8px 8px 0 8px;
}

.container_right_header_image {
    width: 100%;
    height: 120px;
}

.container_right_body_good {
    width: 100%;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.container_right_body_show {
    width: 46%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 0 2%;
}

.container_right_body_good_image {
    width: 100%;
    height: 120px;
}

.container_right_body_title {
    font-size: 16px;
    font-weight: 600;
    margin: 8px 0px 16px 8px;
}

.container_right_body_good_name {
    font-size: 12px;
}

.container_right_none {
    font-size: 12px;
    margin: 8px 8px;
}